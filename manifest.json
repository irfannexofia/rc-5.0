{
  "manifest_version": 3,
  "name": "Royal Checker App",
  "version": "5.0.1",
  "description": "Checker Work Tools",
  "icons": {
    "48": "icons/icon.png"
  },
  "action": {
    "default_title": "Royal Checker App V5.0.1"
  },
  "permissions": [
    "cookies"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_idle",
      "js": [
        "contentScript.js"
      ]
    }
  ]
}